<h1>Merritt - Ingest Workspace Service</h1>

<div id="body">
<div id="menu">
  <a href="/">Home</a>
</div>
<br>
<div>
<form method="POST" action="/listing">
  <input type="submit" value="List"/>
</form>
</div>

<div id="listing">
  <% if @objlist %>
  <h3>Object List</h2>
  <ul>
    <% @objlist.each do |k| %>
      <li><a href="<%= k[:url] %>"><%=k[:key]%></a></li>
    <% end %>
  </ul>
  <% end %>

  <% if @prefixes %>
  <h3>Directories</h2>
  <ul>
    <% @prefixes.each do |k| %>
      <li><a href="<%= k[:url] %>"><%=k[:key]%></a></li>
    <% end %>
  </ul>
  <% end %>
</div>

<div id="manifest">
<h2>Manifest</h2>
<textarea rows=15 cols=120><%=@data%></textarea>
</div>
</div>